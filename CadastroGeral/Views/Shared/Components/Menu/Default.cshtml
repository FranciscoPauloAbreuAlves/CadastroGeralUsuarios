@model UsuarioModel

<a class="navbar-brand" asp-area="" asp-controller="Home" asp-action="Index">Olá, @Model.Nome </a>
 <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target=".navbar-collapse" aria-controls="navbarSupportedContent"
    aria-expanded="false" aria-label="Toggle navigation">
    <span class="navbar-toggler-icon"></span>
 </button>
 <div class="navbar-collapse collapse d-sm-inline-flex justify-content-between">
     <ul class="navbar-nav flex-grow-1">
         <li class="nav-item">
            <a class="nav-link text-light" asp-area="" asp-controller="Home" asp-action="Index">Página Inicial</a>
         </li>
         <!--Contatos-->
         <li class="nav-item">
            <a class="nav-link text-light" asp-area="" asp-controller="Contato" asp-action="Index">Contatos</a>
         </li>
        
        <!--Telas não acessadas-->
        @if(Model.Perfil == CadastroGeral.Enums.UsuarioEnum.Admin)
        {
            <!--usuários-->
            <li class="nav-item">
                <a class="nav-link text-light" asp-area="" asp-controller="Usuario" asp-action="Index">Usuários</a>
            </li>

             <!--Membros
            <li class="nav-item">
            <a class="nav-link text-light" asp-area="" asp-controller="Membro" asp-action="Index">Membros</a>
            </li>-->
        }    
         
         <!--Tarefas-->
         <li class="nav-item">
            <a class="nav-link text-light" asp-area="" asp-controller="Tarefa" asp-action="Index">Tarefas</a>
         </li>

         <!--Alterar senha-->
         <li class="nav-item">
            <a class="nav-link text-light" asp-area="" asp-controller="AlterarSenha" asp-action="Index">Alterar Senha</a>
         </li>

         <!--Sair Login-->
         <li class="nav-item">
            <a class="nav-link text-light" asp-area="" asp-controller="Login" asp-action="Sair">Sair</a>
         </li>
     </ul>
 </div>